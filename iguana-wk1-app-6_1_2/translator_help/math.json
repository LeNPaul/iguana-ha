   "frexp": {
      "Title": "math.frexp",
      "Parameters": [
         {
            "x": {
               "Desc": "The number to calculate from <u>number</u>."
            }
         }
      ],
      "Returns": [
         {
            "Desc": "The number <code>m</code> to be multiplied by <em>2<sup>e</sup></em> (i.e., <em>m2<sup>e</sup></em>) <u>number</u>."
         },
         {
            "Desc": "A number <code>e</code> to be used as the power of 2 (i.e., <em>m2<sup>e</sup></em>) <u>number</u>."
         }
      ],
      "SummaryLine": "Calculates <code>m</code> and <code>e</code> such that <em>x = m2<sup>e</sup></em>.",
      "Usage": "math.frexp(x)",
      "SeeAlso": [
         {
            "Title": "The math module - mathematical functions",
            "Link": "http://help.interfaceware.com/api/?v=6.0.0#math"
         },
         {
            "Title": "Online documentation for math.frexp.",
            "Link": "http://www.lua.org/manual/5.1/manual.html#pdf-math.frexp"
         }
      ],
      "Desc": "Calculates <code>m</code> and <code>e</code> such that <em>x = m2<sup>e</sup></em>, <code>e</code> is an integer and the absolute value of <code>m</code> is in the range <em>[0.5, 1)</em> (or zero when <code>x</code> is zero)."
   },
